#: env/lib/python3.7/site-packages/django/contrib/postgres/apps.py:45
msgid "PostgreSQL extensions"
msgstr "PostgreSQL rozšírenia"

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/array.py:21
#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/array.py:16
#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/array.py:170
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "%(nth)s. položka poľa je neplatná:"

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Vnorené polia musia mať rovnakú dĺžku."

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Mapovanie reťazcov na reťazce/NULL"

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Hodnota s kľúčom \"%(key)s\" nie je reťazec ani NULL."

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/hstore.py:16
msgid "Could not load JSON data."
msgstr "Údaje typu JSON sa nepodarilo načítať."

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "Vstup musí byť slovník vo formáte JSON."

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:33
msgid "Enter two valid values."
msgstr "Zadajte dve platné hodnoty."

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:34
msgid "The start of the range must not exceed the end of the range."
msgstr "Začiatočná hodnota rozsahu nesmie býť vyššia ako koncová hodnota."

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:82
msgid "Enter two whole numbers."
msgstr "Zadajte dve celé čísla."

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:88
msgid "Enter two numbers."
msgstr "Zadajte dve čísla."

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:94
msgid "Enter two valid date/times."
msgstr "Zadajte dva platné dátumy/časy."

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:100
msgid "Enter two valid dates."
msgstr "Zadajte dva platné dátumy."

