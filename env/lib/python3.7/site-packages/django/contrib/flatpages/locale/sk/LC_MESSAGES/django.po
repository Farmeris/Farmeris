#: env/lib/python3.7/site-packages/django/contrib/flatpages/admin.py:12
msgid "Advanced options"
msgstr "Pokročilé nastavenia"

#: env/lib/python3.7/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Statické stránky"

#: env/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:10
#: env/lib/python3.7/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: env/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:13
#, fuzzy
#| msgid ""
#| "Example: '/about/contact/'. Make sure to have leading and trailing "
#| "slashes."
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Príklad: '/about/contact/'. Uistite sa, že máte lomky na začiatku a na konci."

#: env/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:16
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Táto hodnota musí obsahovať len písmená, číslice, bodky, podčiarovníky, "
"pomlčky, lomky alebo vlnovky."

#: env/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:30
#, fuzzy
#| msgid ""
#| "Example: '/about/contact/'. Make sure to have leading and trailing "
#| "slashes."
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr ""
"Príklad: '/about/contact/'. Uistite sa, že máte lomky na začiatku a na konci."

#: env/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:43
msgid "URL is missing a leading slash."
msgstr "V URL chýba úvodná lomka."

#: env/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:48
msgid "URL is missing a trailing slash."
msgstr "V URL chýba koncová lomka."

#: env/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:65
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Statická stránka s url %(url)s pre sídlo %(site)s už existuje."

#: env/lib/python3.7/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "názov"

#: env/lib/python3.7/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "obsah"

#: env/lib/python3.7/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "povoliť komentáre"

#: env/lib/python3.7/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "názov šablóny"

#: env/lib/python3.7/site-packages/django/contrib/flatpages/models.py:18
#, fuzzy
#| msgid ""
#| "Example: 'flatpages/contact_page.html'. If this isn't provided, the "
#| "system will use 'flatpages/default.html'."
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Príklad: 'flatpages/contact_page.html'. Ak nič nenapíšete, systém použije "
"'flatpages/default.html'."

#: env/lib/python3.7/site-packages/django/contrib/flatpages/models.py:23
